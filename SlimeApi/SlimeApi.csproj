<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net7.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Title>SlimeApi</Title>
    <Authors>CosmoKotik</Authors>
    <Company>KottApps</Company>
    <PackageProjectUrl>https://cosmokott.ru/</PackageProjectUrl>
    <Description>This is the official api for minecraft slimecore.</Description>
    <Copyright>GNU GPLv3</Copyright>
    <RepositoryUrl>https://github.com/CosmoKotik/SlimeApi</RepositoryUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <None Update="README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
